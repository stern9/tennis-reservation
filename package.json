{
  "name": "tennis-court-reservation",
  "version": "1.0.0",
  "description": "Automated tennis court reservation system for Parques del Sol",
  "main": "scripts/reserve.js",
  "scripts": {
    "build": "tsc",
    "reserve": "npm run build && node dist/scripts/reserve.js",
    "reserve:test": "npm run build && node dist/scripts/reserve.js --test",
    "reserve:dry-run": "npm run build && node dist/scripts/reserve.js --dry-run",
    "reserve:debug": "npm run build && node dist/scripts/reserve.js --test --debug",
    "reserve:watch": "npm run build && node dist/scripts/reserve.js --test --debug --watch",
    "reserve:shadow": "npm run build && node dist/scripts/reserve.js --test --shadow",
    "reserve:canary": "npm run build && node dist/scripts/reserve.js --test --canary",
    "reserve:mock-unlock": "npm run build && node dist/scripts/reserve.js --test --mock-unlock",
    "diagnose": "node scripts/diagnose-dates.js",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\""
  },
  "keywords": [
    "automation",
    "reservation",
    "tennis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "playwright": "^1.45.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0"
  }
}
